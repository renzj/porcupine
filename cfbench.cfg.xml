   </generator>
    <generator name = 'possion'>
        <path>/root/cfbench/request_generator</path><!-- this path you need to change your path-->
        <direct>request_generator_impl</direct>
        <class_name>PossionRequestGenerator</class_name>
   </generator>

   <iat_distr name='pareto'>
        <path>/root/cfbench/iat_distr/</path><!-- this path you need to change your path-->
        <direct>iat_distribution_impl</direct>
        <class_name>ParetoIATGenerator</class_name>
   </iat_distr>

